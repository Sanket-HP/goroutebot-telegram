{
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/tracker.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/webhook",
      "dest": "index.js"
    }
  ],
  "crons": [
    {
      "path": "/api/cron/tracking",
      "schedule": "0,15,30,45 * * * *"
    }
  ],
  "rewrites": [
    {
      "source": "/api/cron/tracking",
      "destination": "/api/tracker.js"
    }
  ]
}
